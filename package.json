{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning Express and MongoDB",
  "license": "ISC",
  "author": "Kwatpan Dalang",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/js",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js --no-source-maps"
  },
  "parcel-namer-rewrite": {
    "rules": {
      "index.js": "bundle.js"
    },
    "hashing": "never"
  },
  "targets": {
    "default": {
      "distDir": "./public/js",
      "context": "browser",
      "includeNodeModules": true
    }
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "browserslist": "^4.28.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "core-js": "^3.46.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^3.15.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.17.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "pug": "^3.0.3",
    "regenerator-runtime": "^0.14.1",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "stripe": "^20.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@eslint/config-array": "^0.21.0",
    "@eslint/js": "^9.34.0",
    "@eslint/object-schema": "^2.1.6",
    "@parcel/core": "^2.16.3",
    "@parcel/transformer-js": "^2.16.3",
    "@parcel/transformer-pug": "^2.16.0",
    "buffer": "^6.0.3",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "glob": "^9.3.5",
    "globals": "^16.3.0",
    "parcel": "^2.16.3",
    "parcel-namer-rewrite": "^2.10.3-rc.5",
    "prettier": "^3.6.2",
    "rimraf": "^4.4.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
